<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mservice.msg_web.mapper.DaehoUserMapper">
    <select id="selectUserById" resultType="com.mservice.msg_web.model.DaehoUser">
        SELECT
            id AS id,
            password AS password
        FROM
            daeho_user
    </select>

    <insert id="insert" parameterType="com.mservice.msg_web.model.DaehoUser">
        INSERT INTO daeho_user (
            id,
            password
        )
        VALUES (
            #{id},
            #{password}
        )
    </insert>
</mapper>